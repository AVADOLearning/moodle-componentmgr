#!/bin/sh

#
# Moodle component manager.
#
# @author Luke Carrier <luke@carrier.im>
# @copyright 2016 Luke Carrier
# @license GPL-3.0+
#

binDir="$(dirname "$(readlink -fn "$0")")"
rootDir="$(dirname "$binDir")"

php -d variables_order=EGPCS \
    -d error_reporting=-1 \
    -d display_errors=On \
    "${rootDir}/libexec/componentmgr" ${@}
errno=$?

exit $errno
